Program Median
kamus 
    i, y, bilangan, total : integer
algoritma 
    input(y)
    total = 0
    for i = 1 to 9 do
        input(bilangan)
        total = total + bilangan
    end for
    if total >= y * 5 then
        output("Median bernilai", y)
    else
        output("Median bernilai 0")
    end if
endprogram